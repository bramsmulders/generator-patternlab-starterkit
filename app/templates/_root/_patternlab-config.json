{
    "paths" : {
        "source" : {
            "root": "./<%= answers.sourcePath %>/",
            "patterns" : "./<%= answers.sourcePath %>/_patterns/",
            "data" : "./<%= answers.sourcePath %>/_data/",
            "meta": "./<%= answers.sourcePath %>/_meta/",
            "annotations" : "./<%= answers.sourcePath %>/_annotations/",
            "styleguide" : "./node_modules/styleguidekit-assets-default/dist/",
            "patternlabFiles" : "./node_modules/styleguidekit-mustache-default/views/",
            "js" : "./<%= answers.sourcePath %>/js",
            "images" : "./<%= answers.sourcePath %>/images",
            "fonts" : "./<%= answers.sourcePath %>/fonts",
            "css" : "./<%= answers.sourcePath %>/css/"
        },
        "public" : {
            "root" : "./<%= answers.publicPath %>/",
            "patterns" : "./<%= answers.publicPath %>/patterns/",
            "data" : "./<%= answers.publicPath %>/styleguide/data/",
            "annotations" : "./<%= answers.publicPath %>/annotations/",
            "styleguide" : "./<%= answers.publicPath %>/styleguide/",
            "js" : "./<%= answers.publicPath %>/js",
            "images" : "./<%= answers.publicPath %>/images",
            "fonts" : "./<%= answers.publicPath %>/fonts",
            "css" : "./<%= answers.publicPath %>/css"
        }
    },
    "styleGuideExcludes": [
    ],
    "defaultPattern": "all",
    "defaultShowPatternInfo": false,
    "cleanPublic" : true,
    "patternExtension": "mustache",
    "ignored-extensions" : ["scss", "DS_Store", "less"],
    "ignored-directories" : ["scss"],
    "debug": false,
    "ishControlsHide": {
        "s": false,
        "m": false,
        "l": false,
        "full": false,
        "random": false,
        "disco": false,
        "hay": true,
        "mqs": false,
        "find": false,
        "views-all": false,
        "views-annotations": false,
        "views-code": false,
        "views-new": false,
        "tools-all": false,
        "tools-docs": false
    },
    "ishMinimum": "240",
    "ishMaximum": "2600",
    "patternStateCascade": ["inprogress", "inreview", "complete"],
    "patternStates": {
    },
    "patternExportPatternPartials": [],
    "patternExportDirectory": "./pattern_exports/",
    "cacheBust": true,
    "starterkitSubDir": "dist",
    "outputFileSuffixes": {
        "rendered": ".rendered",
        "rawTemplate": "",
        "markupOnly": ".markup-only"
    },
    "cleanOutputHtml": true
}
